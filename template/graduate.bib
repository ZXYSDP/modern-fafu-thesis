@article{panConstructionExperimentalVerification2021,
  title = {Construction and Experimental Verification of Sloped Terrain Soil Pressure-Sinkage Model},
  author = {Pan, Guanting and Sun, Jingbin and Wang, Xiaole and Yang, Fuzeng and Liu, Zhijie},
  year = {2021},
  month = mar,
  journal = {Agriculture},
  volume = {11},
  number = {3},
  pages = {243},
  issn = {2077-0472},
  doi = {10.3390/agriculture11030243},
  urldate = {2023-12-20},
  abstract = {The construction of a scientific and effective soil pressure-sinkage model under sloped terrain condition has important guiding significance for the investigation of the soil compaction effect. It is also important for the theoretical calculation of driving resistance and design optimization of the undercarriage structure of hillside metal-tracked tractors (HMTs). The classic Bekker's pressure-sinkage model does not consider the influence of the soil water content, bulk density, slope angle, and other factors; therefore, it cannot be directly used to investigate the relationship between the soil compaction and its sinkage under sloped terrain conditions. To solve this problem, this study first verified that the soil water content and bulk density exert significant effects on the pressure--sinkage relationship under flat terrain condition. Secondly, a pressure-sinkage test was carried out using the quadratic rotation orthogonal combination design method, and the soil water content, density, and slope angle were considered. The pressure-sinkage curves of sloped terrain soils from Yangling and Yangxian in Shaanxi Province, and Huining and Jingning in Gansu Province were obtained. Then the pressure--sinkage parameters (sinkage exponent, cohesive modulus, and frictional modulus) were calculated using the weighted least-squares method. Thirdly, the mathematical relationship between the parameters and the soil water content, bulk density, and slope angle was obtained. Then Bekker's model was modified to obtain the pressure--sinkage model of sloped terrain. Finally, the control variable method under slope angle of 10{$^\circ$}, soil water content of 10\%, and bulk density of 2 mg{$\cdot$}m-3 were used to validate the model. The results revealed that the root-mean-square error between the calculated pressure value of the model and the measured value of the film pressure sensor was 1.614, 1.601, and 0.822, respectively. In the dynamic operation of a hillside tractor prototype, the calculated pressures between the supporting wheels were close to the measured values. It indicates that the modified soil pressure--sinkage model is more suitable for calculating the force at the bottom of the track between the supporting wheels. It can also provide an important theoretical basis for accurately calculating the pressure--sinkage parameters of sloped terrain soil. Additionally, this approach could provide theoretical and technical support for the rational arrangement of HMT undercarriages to reduce the soil sinkage and driving resistance.},
  langid = {english},
  file = {C:\Users\ZXYSDP\Zotero\storage\JI65KMBS\Pan 等 - 2021 - Construction and Experimental Verification of Slop.pdf}
}

@phdthesis{PanShanDiLuDaiTuoLaJiPoDiZhuanXiangDongTaiWenDingXingLiLunYanJiu2016,
  type = {{硕士学位论文}},
  title = {{山地履带拖拉机坡地转向动态稳定性理论研究}},
  author = {潘, 冠廷},
  year = {2016},
  address = {咸阳},
  urldate = {2023-12-29},
  abstract = {丘陵山区地形复杂,耕地小而分散,地面高差大,使得农业机械的作业相比在平原作业更加困难。山地履带拖拉机作为一种适用于我国丘陵山区农业耕作的动力机械,不仅应具有一般拖拉机的基本特点,而且应具有良好的坡地工作性能,尤其是坡地转向稳定性。随着我国对丘陵山区农业机械化发展的日益重视,丘陵山区农业机械需求量日益增加,开展对山地履带拖拉机坡地转向稳定性的研究,对于改善其整机性能起着不可替代的作用。山地履带拖拉机是一个比较复杂的机械系统。基于本实验室(农业部北方农业装备科学观测实验站)自行研制的山地履带拖拉机样机,本文综合采用经典的力学分析法、动力学分析法等,以匀速、低速转向为前提,对履带拖拉机在坡地上稳定转向机理进行研究。主要研究内容和结果如下:(1)运用经典力学理论,对山地履带拖拉机进行坡道直线行驶的动力学分析,讨论了其在不同行驶方向、是否带有悬挂机具等条件下的极限倾翻角与极限下滑角。结果表明,拖拉机重心高度、其重心至履带支撑段后缘的距离、农具重量、农具重心至履带支撑段后缘的距离等直接影响山地履带拖拉机的极限倾翻角与极限下滑角。该山地履带拖拉机直线上坡的极限倾翻角为52.3{$^\circ$},直线下坡的极限倾翻角为54.1{$^\circ$},而其极限下滑角仅与履带和路面之间的附着系数有关;(2)首次以坡地角度、低速转向为条件,将一般的履带车辆高速转向理论引入山地履带拖拉机的转向稳定性研究,在忽略高速转向影响因素的前提下,建立了拖拉机在坡地转向时履带接地段的接地比压,从而求解出履带驱动力、转向阻力等与拖拉机设计参数、地面参数和拖拉机转向基点偏移量之间的函数关系;应用RecurDyn软件针对特定条件下的履带拖拉机进行坡地转向仿真,仿真结果表明,履带所受纵向力变化趋势与理论推导得出的变化趋势基本吻合,因此,该坡地转向理论具有一定的可行性;(3)对山地履带拖拉机在坡道上直线行驶时的转向瞬态进行力学分析,通过建立线性分布的履带接地段法向负荷,推导了其在不同行驶和转向方向状态下,瞬时转向基点偏移量的表达式,得出了影响其在相应转向状态下稳定性的因素和影响规律,并得到了最大转向基点偏移量。结果表明,该山地履带拖拉机上坡时的最大转向基点偏移量为0.61m,下坡时最大转向基点偏移量为0.79m。},
  langid = {chinese},
  school = {西北农林科技大学}
}

@article{PanXiaoXingShanDiLuDaiTuoLaJiPaPoYueZhangXingNengFenXiYuShiYan2020,
  title = {{小型山地履带拖拉机爬坡越障性能分析与试验}},
  author = {潘, 冠廷 and 杨, 福增 and 孙, 景彬 and 刘, 志杰},
