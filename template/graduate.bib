@article{LiuBanZhuDongXuanJiaShanDiTuoLaJiZiTaiKongZhiXiTongSheJiYuFangZhen2022,
  title = {{半主动悬架山地拖拉机姿态控制系统设计与仿真}},
  author = {刘, 国辉 and 郝, 称意 and 李, 民赞 and 孙, 红},
