@inproceedings{richierDualBacksteppingObserver2012,
  title = {Dual Back-Stepping Observer to Anticipate the Rollover Risk in under/over-Steering Situations. {{Application}} to {{ATVs}} in off-Road Context},
  booktitle = {2012 {{IEEE}}/{{RSJ International Conference}} on {{Intelligent Robots}} and {{Systems}}},
  author = {Richier, Mathieu and Lenain, Roland and Thuilot, Benoit and Debain, Christophe},
