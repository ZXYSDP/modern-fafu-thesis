@article{huAnalysisDesignAutoadaptive2021,
  title = {Analysis and Design of Auto-Adaptive Leveling Hydraulic Suspension for Agricultural Robot},
  author = {Hu, Kai and Zhang, Wenyi and Qi, Bing},
  year = {2021},
