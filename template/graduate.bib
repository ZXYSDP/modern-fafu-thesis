@article{liuDesignMatchingDynamic2021,
  title = {Design Matching and Dynamic Performance Test for an {{HST-based}} Drive System of a Hillside Crawler Tractor},
  author = {Liu, Zhijie and Zhang, Guoqiang and Chu, Guoping and Niu, Hanlin and Zhang, Yazhou and Yang, Fuzeng},
  year = {2021},
  month = may,
  journal = {Agriculture},
